clc
u=(randn(1000,1));
polos=[0.9 0.8 0.7];

yi=filter([0 prod(1-polos)],poly(polos),u);

T=[];
for i=1:1

vi=randn(1000,1)*0.1;
y=yi+vi;

tao=sib_arx(u,y,3,2,0);
ta=tao*0.9;

tic()
[t] = sib_oe_conjugate5(u,y,tao,0,2) 
toc()
T=[T t];


end

figure(2)
plot(T(1,:),T(2,:),'o')
figure(3)
plot(T(3,:),T(4,:),'o')


figure(1)
Q=60
G=tf(T(1,Q),[1 T(2:4,Q)'],1);
step(G);
hold on
G1=tf(T(1,1),[1 T(2:4,1)'],1);
step(G1);
G1=tf(T(1,1),[1 T(2:4,1)'],1);
step(G1);
hold off